<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>



        function delayPromise() {
            return new Promise( (require) => {
                const fs = require('fs')

                fs.readFile('foo.txt', (err, data) => {
                    if (err) throw err;
                    console.log( data.toString() );
                    return data
                });

             })
            
            
        }

        async function main() {

            console.log('start')
            await delayPromise()
            console.log('bottom of program')
        }
        main()
        
    </script>
</body>
</html>